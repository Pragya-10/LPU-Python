{
    "version": "3.2",
    "services": {
        "csflix": {
            "image": "jatinkatyal13/csflix",
            "env_file": ".env",
            "ports": [
                "8000:8000"
            ]
        },
        "minio": {
            "image": "minio/minio",
            "ports": [
                "9000:9000"
            ],
            "environment": {
                "MINIO_ROOT_USER": "admin",
                "MINIO_ROOT_PASSWORD": "adminadmin"
            },
            "command": "server /data"
        }
    }
}
